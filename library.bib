issn = {13882457},
journal = {Clin. Neurophysiol.},
keywords = {alzheimer,approximation,fast fourier transformation dipole,mild cognitive impairment,quantitative electroencephalography,s disease},
month = {nov},
number = {11},
pages = {1961--1967},
pmid = {11068230},
title = {{Discrimination of Alzheimer's disease and mild cognitive impairment by equivalent EEG sources: a cross-sectional and longitudinal study}},
