@article{Fazli2009a,
abstract = {Current state-of-the-art in Brain Computer Interfacing (BCI) involves tuning classifiers to subject-specific training data acquired from calibration sessions prior to functional BCI use. Using a large database of EEG recordings from 45 subjects, who took part in movement imagination task experiments, we construct an ensemble of classifiers derived from subject-specific temporal and spatial filters. The ensemble is then sparsified using quadratic regression with ???1 regularization such that the final classifier generalizes reliably to data of subjects not included in the ensemble. Our offline results indicate that BCI-na??ve users could start real-time BCI use without any prior calibration at only very limited loss of performance. ?? 2009 Elsevier Ltd. All rights reserved.},
