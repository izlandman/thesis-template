issn = {08852308},
journal = {Comput. Speech Lang.},
keywords = {age recognition,formant,gender recognition,gmm,harmonic structure,linear regression,maximum likelihood,pitch,polynomial expansion,prosodic features,score level fusion,sparse representation,supervectors,svm,ubm weight posterior probability},
