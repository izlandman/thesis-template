issn = {01676393},
journal = {Speech Commun.},
keywords = {Age of entry,Foreign accent recognition,Language proficiency,Level of education,Where second language is spoken,i-Vector},
month = {feb},
pages = {118--129},
title = {{Factors affecting i-vector based foreign accent recognition: A case study in spoken Finnish}},
