isbn = {2162-237X},
issn = {2162-2388},
journal = {IEEE Trans. neural networks Learn. Syst.},
keywords = {Brain-computer interface (BCI),electroencephalogram (EEG),recurrent quantum neural network (RQNN)},
month = {feb},
