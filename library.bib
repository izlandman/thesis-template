journal = {IEEE Trans. Inf. Forensics Secur.},
keywords = {EEG,biometrics,brain rhythms,elicitation protocols},
number = {5},
pages = {782--800},
title = {{Brain waves for automatic biometric-based user recognition}},
volume = {9},
year = {2014}
