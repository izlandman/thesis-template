file = {:C$\backslash$:/Users/seadubbs/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gaspard et al. - 2014 - Interrater agreement for Critical Care EEG Terminology.pdf:pdf},
isbn = {1528-1167 (Electronic)$\backslash$r0013-9580 (Linking)},
