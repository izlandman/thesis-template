issn = {2168-2208},
journal = {IEEE J. Biomed. Heal. informatics},
keywords = {Artifact removal,Biorthogonal wavelet,Electroencephalogram},
month = {jan},
number = {1},
pages = {158--65},
pmid = {24968340},
title = {{Unsupervised eye blink artifact denoising of EEG data with modified multiscale sample entropy, Kurtosis, and wavelet-ICA.}},
