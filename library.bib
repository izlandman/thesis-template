file = {:C$\backslash$:/Users/seadubbs/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rakthanmanon et al. - 2012 - Searching and mining trillions of time series subsequences under dynamic time warping.pdf:pdf},
isbn = {9781450314626},
issn = {10450823},
journal = {Proc. 18th ACM SIGKDD Int. Conf. Knowl. Discov. Data Min.},
keywords = {lower bounds,similarity search,time series},
pages = {262--270},
title = {{Searching and mining trillions of time series subsequences under dynamic time warping}},
url = {http://dl.acm.org/citation.cfm?id=2339576{\%}5Cnhttp://practicalquant.blogspot.com/2012/10/mining-time-series-with-trillions-of.html},
